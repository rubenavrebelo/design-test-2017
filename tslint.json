{
    "extends": ["tslint:latest", "tslint-react"],
    "rules": {
      // override tslint-react rules here
      "jsx-wrap-multiline": false,
      "ordered-imports": false,
      "quotemark": [true, "single", "jsx-single"],
      "member-access": false,
      "no-trailing-whitespace": false,
      "eofline": false,
    }
}